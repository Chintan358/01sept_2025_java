package array;

public class A006_MDArray {
	public static void main(String[] args) {
		
		
//		int a[][] = new int[5][3];
//		
//		a[0][0] = 45;
//		a[0][1] = 46;
//		a[0][2] = 13;
//		
//		a[1][0] = 45;
//		a[1][1] = 46;
//		a[1][2] = 13;
		
		int a[][] = {{10,20,30},{10,20,30},{45,48,65}};
		
		for(int i=0;i<3;i++)
		{
			for(int j=0;j<3;j++)
			{
				System.out.print(a[i][j]+" ");
			}
			
			System.out.println();
		}
		
	}
}
