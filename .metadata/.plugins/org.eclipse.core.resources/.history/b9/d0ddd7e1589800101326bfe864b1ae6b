package array;

public class A005_sorting {
	public static void main(String[] args) {
		
		
		
		int a[] = {10,40,5,68,64,45,98,12,36,98,12,70};
		
		
		
		for(int i=0;i<a.length;i++)
		{
			for(int j=i;j<a.length;j++)
			{
				if(a[j]>a[i])
				{
					int temp = a[j];
					a[i] = a[j];
					a[j] = temp;
				}
			}
		}
		
		for (int i = 0; i < a.length; i++) {
			System.out.println(a[i]);
		}
		
	}
}
